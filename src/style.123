* {
	font-family: "TT Lakes";
}
.navigation-button {
	width: 40px;
	height: 40px;
	background-color: #FF3E79;
	border-radius: 50%;
	opacity: 1;
	transition: all 0.3s;
	&:hover {
		transform: scale(1.2);
		opacity: 0.8;
	}
	&:active {
		transform: scale(1.3);
		opacity: 1;
		transition: 0.05s;
	}
}
.navigation-button__elem {
	border: none;
	background: none;
	background-repeat: no-repeat;
	width: 100%;
	height: 100%;
	padding: 0%;
	background-position: center;
	opacity: 1;
	transition: all 0.3s;
}
.header-navigation__buttons--left {
	display: flex;
	align-items: center;
	gap: 16px;
}
.header-navigation__buttons--right {
	display: flex;
	align-items: center;
	gap: 16px;
}
.header-navigation__buttons--hidden {
	display: flex;
	align-items: center;
	gap: 16px;
}
.navigation-button__elem--repair {
	background-image: url(../img/repair.svg);
}
.navigation-button__elem--checkstatus {
	background-image: url(../img/search.svg);
}
.navigation-button__elem--profile {
	background-image: url(../img/profile.svg);
}
.navigation-button__elem--burger {
	background-image: url(../img/burger-close.svg);
}
.navigation-button__elem--chat {
	background-image: url(../img/chat.svg);
}
.navigation-button__elem--call {
	background-image: url(../img/call.svg);
}
.navigation-button__elem--burger-open {
	background-image: url(../img/burger.svg);
}
.navigation-button__elem--search {
	background-image: url(../img/loop.svg);
}
.description__wrapper--open {
	max-height: fit-content !important;
}
body {
	margin: 0 auto;
}
.header-navigation {
	height: 88px;
	display: flex;
	justify-content: space-between;
	border-bottom: #d9fff5 solid 1px;
	align-items: center;
}
.feedback-button {
	height: 40px;
	width: 91px;
	font-size: 16px;
	line-height: 24px;
	color: #7e7e82;
	text-align: left;
	background: none;
	border: none;
	padding: 0 1em;
	&:focus {
		border-radius: 6px;
		border: 2px solid #88ffec;
		background-color: #fff;
	}
}
.call-button {
	height: 40px;
	width: 91px;
	font-size: 16px;
	line-height: 24px;
	color: #7e7e82;
	text-align: left;
	background: none;
	border: none;
	padding: 0 1em;
	&:focus {
		border-radius: 6px;
		border: 2px solid #88ffec;
		background-color: #fff;
	}
}
.container {
	margin: 0 auto;
}
.info__title-line {
	position: absolute;
}
.info__title {
	font-size: 28px;
	line-height: 40px;
}
.info__navigation {
	font-size: 16px;
	display: flex;
}
.navigation__button {
	border: hidden;
	border-radius: 6px;
	background: #F8F8F8;
	font-size: 16px;
	line-height: 24px;
	letter-spacing: -0.2px;
	color: #7e7e82;
	height: 40px;
	white-space: nowrap;
	padding: 0 1em;
	cursor: pointer;
	transition: 0.2s;
	&:hover {
		border: #7E7E82 2px solid;
	}
	&:focus {
		border-radius: 6px;
		border: 2px solid #88ffec;
		background-color: #fff;
	}
}
.description__text {
	margin: 0 0 16px;
	font-size: 14px;
	letter-spacing: 0.2px;
	color: #1b1c21;
	line-height: 18px;
}
.text-expander {
	margin-top: 16px;
	font-weight: 600;
	color: #1b1c21;
	font-size: 16px;
	line-height: 24px;
	border: hidden;
	background-color: #F8F8F8;
	&::before {
		content: url(../img/expand.svg);
		width: 24px;
		height: 24px;
		vertical-align: middle;
		display: inline-block;
	}
	&:focus {
		border-radius: 6px;
		border: 2px solid #88ffec;
		margin-top: 14px;
		margin-left: -2px;
		background-color: #fff;
	}
	&:hover {
		transition: 0.2s;
		transform: scale(1.1);
	}
}
.text-expander--active {
	&::before {
		content: url(../img/expand.svg);
		width: 24px;
		height: 24px;
		vertical-align: middle;
		display: inline-block;
		transform: rotate(180deg);
	}
}
.aside-menu__navigation-menu__list {
	li {
		a {
			&:hover {
				transition: 0.2s;
				border-bottom: 6px solid #7E7E82;
				opacity: 0.9;
			}
			text-decoration: none;
			color: #1B1C21;
		}
		margin-bottom: 32px;
		position: relative;
	}
	list-style: none;
	padding: 0;
}
.aside-menu__logo {
	position: absolute;
	left: 60px;
}
.aside-menu__top-header {
	position: relative;
	display: flex;
	justify-content: space-between;
	align-items: center;
	margin: 24px 0 32px 0;
}
.footer__navigation-icons {
	display: flex;
	gap: 16px;
	margin-bottom: 16px;
}
.aside-menu__footer {
	margin-bottom: 32px;
}
.footer-phone {
	margin-top: 12px;
	font-size: 24px;
	font-weight: 600;
	text-decoration: none;
	color: #1b1c21;
	display: block;
}
.footer__language-select {
	li {
		list-style: none;
		display: inline;
		&:focus {
			color: #7e7e82;
		}
	}
	input[type="radio"] {
		visibility: hidden;
		position: absolute;
	}
	input {
		&:checked+label {
			color: #7e7e82;
		}
	}
	>label {
		margin-right: 7px;
		transition: all 0.3s;
	}
	label {
		&:hover {
			cursor: pointer;
		}
	}
}
.footer__navigation {
	margin-bottom: 40px;
}
.aside-menu {
	li {
		a {
			&:focus {
				color: #7e7e82;
			}
		}
	}
}
.footer-email {
	text-decoration: none;
	color: #1b1c21;
	display: block;
}
.navigation-container__expander-button-block {
	margin-top: 16px;
	display: inline-flex;
}
.brands-expander {
	height: 40px;
	position: relative;
	font-weight: 600;
	color: #1b1c21;
	font-size: 16px;
	line-height: 24px;
	border: hidden;
	background-color: inherit;
	&:hover {
		transition: 0.2s;
		transform: scale(1.1);
	}
	&:focus {
		border-radius: 6px;
		border: 2px solid #88ffec;
	}
}
.service-expander {
	height: 40px;
	position: relative;
	font-weight: 600;
	color: #1b1c21;
	font-size: 16px;
	line-height: 24px;
	border: hidden;
	background-color: inherit;
	&:hover {
		transition: 0.2s;
		transform: scale(1.1);
	}
	&:focus {
		border-radius: 6px;
		border: 2px solid #88ffec;
	}
}
.brands-expander__icon--unactive {
	vertical-align: middle;
}
.service-expander__icon--unactive {
	vertical-align: middle;
}
.brands-expander__icon--active {
	vertical-align: middle;
	transform: rotate(180deg);
}
.service-expander__icon--active {
	vertical-align: middle;
	transform: rotate(180deg);
}
.brand-list__elem {
	background-color: #fff;
	border: #eaeaea 1px solid;
	border-radius: 8px;
	position: relative;
	display: block;
	width: 224px;
	height: 72px;
	background-repeat: no-repeat;
	background-position: 20px;
	min-width: 0;
}
.arrow {
	content: '';
	display: inline-block;
	position: relative;
	border: solid #FF3E79;
	border-width: 0 3px 3px 0;
	padding: 3px;
	transform: rotate(-45deg) translate(60%, 180%);
	cursor: pointer;
	border-radius: 1.5px 1.5px 1.5px 1.5px;
}
.circle {
	position: absolute;
	display: block;
	width: 40px;
	height: 40px;
	border: 2px solid #FF3E79;
	border-radius: 100%;
	top: calc(50% + 2px);
	left: calc(85% + 2px);
	transform: translate(-50%, -50%);
	&:hover {
		transition: 0.3s;
		transform: scale(1.06) translate(-50%, -50%);
	}
}
.navigation-container__service-list {
	display: flex;
	flex-wrap: wrap;
	height: 180px;
	overflow-y: hidden;
	padding: 0;
}
.service-list__elem {
	width: 192px !important;
	height: 144px;
	background-color: #fff;
	border: #eaeaea 1px solid;
	border-radius: 8px;
	position: relative;
	padding: 16px;
	color: #1B1C21;
	font-size: 16px;
	font-style: normal;
	font-weight: 500;
	line-height: 24px;
	letter-spacing: -0.2px;
	>.circle {
		transform: translate(-50%, 70%);
		&:hover {
			transform: translate(-50%, 70%) scale(1.1);
		}
	}
}
.swiper-slide.service-list__elem {
	margin-right: 82px !important;
}
.navigation-container__title {
	color: #7e7e82;
	background: linear-gradient(270deg, #FFF 0%, #F8F8F8 100%);
	font-weight: 400;
	font-size: 16px;
	line-height: 24px;
	letter-spacing: -0.2px;
	width: 106%;
}
.navigation-container {
	background-color: #f8f8f8;
	max-width: 100%;
}
.brand-list__elem.item1 {
	background-image: url(../img/Lenovo.png);
}
.brand-list__elem.item2 {
	background-image: url(../img/acer.png);
}
.brand-list__elem.item3 {
	background-image: url(../img/Bosch.png);
}
.brand-list__elem.item4 {
	background-image: url(../img/apple.png);
}
.brand-list__elem.item5 {
