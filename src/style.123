}
.brand-list__elem.item8 {
	background-image: url(../img/HP.png);
}
.brand-list__elem.item9 {
	background-image: url(../img/Lenovo.png);
}
.brand-list__elem.item10 {
	background-image: url(../img/acer.png);
}
.brand-list__elem.item11 {
	background-image: url(../img/Samsung.png);
}
.oneclick-button {
	width: 119px;
	height: 32px;
	background: none;
	background-color: #FF3E79;
	border: none;
	border-radius: 16px;
	&::before {
		content: 'Заказать';
		color: #FFF;
		text-align: right;
		font-size: 12px;
		font-style: normal;
		font-weight: 700;
		line-height: 24px;
		letter-spacing: -0.15px;
		text-transform: uppercase;
	}
	>.arrow {
		content: '';
		display: inline-block;
		position: relative;
		border: solid #fff;
		border-width: 0 3px 3px 0;
		padding: 3px;
		transform: rotate(-45deg) translate(50%, 50%);
		cursor: pointer;
		border-radius: 1.5px 1.5px 1.5px 1.5px;
	}
	&:hover {
		.arrow {
			transition: ease-in-out 0.5s;
			transform: rotate(-45deg) translate(50%, 50%) scale(1.3);
		}
		transition: all 0.1s;
		transform: scale(1.1);
		opacity: 0.9;
	}
	&:active {
		transition: 0.05s;
		opacity: 1;
		transform: scale(1.2);
	}
}
.prices-table {
	width: 100%;
	background-color: #fff;
	border-radius: 6px;
	border-collapse: collapse;
	font-size: 14px;
	font-style: 400;
	font-weight: 500;
	line-height: 16px;
	tr {
		height: 64px;
		border-left: 24px solid #fff;
		border-right: 24px solid #fff;
	}
	td {
		border-bottom: 2px solid #f8f8f8;
	}
}
.prices-table__row {
	&:first-child {
		background-color: #F8F8F8;
		color: #7E7E82;
		border-left: 24px solid #f8f8f8;
		border-right: 24px solid #f8f8f8;
	}
}
.prices-list__elem {
	margin-right: 72px !important;
	width: 192px !important;
	height: 144px;
	background-color: #fff;
	border: #eaeaea 1px solid;
	border-radius: 8px;
	position: relative;
	padding: 20px 32px 20px 12px;
	color: #1B1C21;
	font-size: 16px;
	font-style: normal;
	font-weight: 500;
	line-height: 24px;
	letter-spacing: -0.2px;
	h3 {
		color: #7E7E82;
		opacity: 0.72;
		font-size: 14px;
		font-style: normal;
		font-weight: 500;
		line-height: 16px;
		margin: 0;
	}
	p {
		margin: 0 0 16px;
		color: #1B1C21;
		font-size: 14px;
		font-style: normal;
		font-weight: 400;
		line-height: 18px;
		letter-spacing: 0.2px;
	}
	button {
		position: absolute;
		bottom: 12px;
		right: 12px;
	}
}
.navigation-container__prices-list--mobile {
	padding-left: 20px;
	height: 210px;
}
.order-button__cell {
	display: flex;
	justify-content: flex-end;
	height: 64px;
	align-content: center;
	flex-wrap: wrap;
}
.footnote-block__text {
	color: #1B1C21;
	font-size: 14px;
	font-style: normal;
	font-weight: 400;
	line-height: 20px;
	letter-spacing: 0.2px;
	width: 421px;
	margin-top: 32px;
}
.footnote-block__get-offer {
	color: #1B1C21;
	font-size: 16px;
	font-family: 'TT Lakes';
	font-weight: 500;
	line-height: 24px;
	letter-spacing: -0.2px;
	text-decoration: none;
	&:hover~.arrow {
		transition: all 0.4s;
		transform: rotate(-45deg) translate(50%, 50%) scale(1.3);
	}
}
.footnote-block__get-offer~.arrow {
	transform: rotate(-45deg) translate(20%, 20%);
}
.footer__copyrights {
	color: #7E7E82;
	font-size: 14px;
	font-style: normal;
	font-weight: 500;
	line-height: 16px;
	text-decoration: none;
	opacity: 0.72;
	margin: 65px 0 0;
	padding-bottom: 16px;
	display: flex;
	a {
		color: #7E7E82;
		font-size: 14px;
		font-style: normal;
		font-weight: 500;
		line-height: 16px;
		text-decoration: none;
		opacity: 0.72;
	}
	p {
		color: #7E7E82;
		font-size: 14px;
		font-style: normal;
		font-weight: 500;
		line-height: 16px;
		text-decoration: none;
		opacity: 0.72;
		&:last-child {
			text-align: end;
		}
	}
}
.swiper-pagination-bullet {
	width: 12px;
	height: 12px;
}
.navigation-container__service-list--mobile {
	padding-left: 20px;
	height: 210px;
}
.modal__feedback {
	display: flex;
	justify-content: flex-end;
	min-width: 100%;
	max-width: 100%;
	min-height: 100%;
	background-color: rgba(234, 234, 234, 0.9);
	position: fixed;
	z-index: 99999;
	transition: all 0.5s;
}
.modal__call {
	display: flex;
	justify-content: flex-end;
	min-width: 100%;
	max-width: 100%;
	min-height: 100%;
	background-color: rgba(234, 234, 234, 0.9);
	position: fixed;
	z-index: 99999;
	transition: all 0.5s;
}
.feedback__drawer {
	background-color: #fff;
	display: flex;
	flex-direction: column;
	transition: ease-in-out 0.2s;
	.feedback-drawer-button__elem--burger {
		background-image: url(../img/burger.svg);
	}
}
.call__drawer {
	background-color: #fff;
	display: flex;
	flex-direction: column;
	transition: ease-in-out 0.2s;
	.call-drawer-button__elem--burger {
		background-image: url(../img/burger.svg);
	}
}
.drawer__input {
	margin-bottom: 16px;
	height: 48px;
	padding: 12px 16px 0;
	border-radius: 8px;
	border: 1px solid #EAEAEA;
	background: #FFF;
	font-size: 16px;
	font-style: normal;
	font-weight: 500;
	line-height: 24px;
	letter-spacing: -0.2px;
	overflow-wrap: break-word;
	resize: none;
	text-align: start;
	&:last-child {
		height: 118px;
	}
	&:focus-visible {
		outline: none;
	}
	&::placeholder {
		color: #B5B6BC;
	}
}
.drawer__footnote {
	color: #7E7E82;
	font-size: 14px;
	font-style: normal;
	font-weight: 500;
	line-height: 16px;
	text-align: justify;
	margin-bottom: 32px;
	a {
		color: #FF3E79;
		text-decoration: none;
	}
}
.drawer__send-button {
	&::before {
		content: 'Отправить';
	}
	align-self: flex-end;
}
.modal__feedback--close {
	opacity: 0;
	z-index: -10;
}
.modal__call--close {
	opacity: 0;
	z-index: -10;
}
.modal__feedback--open {
	opacity: 1;
	z-index: 50;
}
.modal__call--open {
	opacity: 1;
	z-index: 50;
}
.feedback__drawer--close {
	transition: ease-in 0.2s;
	transform: translateX(900px) scaleX(0);
}
.call__drawer--close {
	transition: ease-in 0.2s;
	transform: translateX(900px) scaleX(0);
}
.feedback__drawer--open {
	z-index: 100;
	transition: ease-in 0.5s;
	transform: translateX(0px) scale(1);
}
.call__drawer--open {
	z-index: 100;
	transition: ease-in 0.5s;
	transform: translateX(0px) scale(1);
}
.modal__feedback--close-area {
	position: absolute;
	width: 100%;
	height: 100%;
}
.modal__call--close-area {
	position: absolute;
	width: 100%;
	height: 100%;
}
@media (min-width: 1px) {
	.container {
		width: 320px;
		background-color: #F8F8F8;
	}
	.modal__feedback--close-area {
		display: none;
	}
	.modal__call--close-area {
		display: none;
	}
	.aside-menu {
		max-width: 360px;
		min-width: 288px;
		display: flex;
		flex-direction: column;
		justify-content: space-between;
		box-shadow: 25px 0px 50px 10px rgba(87, 85, 109, 0.2);
		position: fixed;
		background-color: #fff;
		padding: 0 16px;
		height: 100%;
	}
	.aside-wrapper {
		transition: ease-in-out 0.7s;
		position: relative;
		z-index: 10;
	}
	.aside-wrapper--close {
		left: -2000px;
		opacity: 0;
	}
	.aside-menu__navigation-menu__list {
		li {
			a {
				&:focus {
					&::before {
						content: url(../img/highlight.svg);
						position: absolute;
						left: -16px;
						top: -4px;
					}
				}
			}
		}
	}
	.navigation-container__prices {
		display: none;
	}
	.mobile-slider {
		display: block;
	}
	.content-wrapper {
		padding: 0 16px;
		background-color: #F8F8F8;
	}
	.navigation-container__brands-list--mobile {
		height: 120px;
		padding-left: 16px;
	}
	.navigation-container__brands-list {
		display: none;
	}
	.navigation-container__wrapper {
		display: none;
	}
	.info__navigation {
		overflow-x: scroll;
	}
	.header-navigation__buttons--hidden {
		visibility: hidden;
		position: absolute;
	}
	.divider--hidden {
		visibility: hidden;
		position: absolute;
	}
	.header-navigation__buttons__desktop {
		visibility: hidden;
		position: absolute;
	}
	.navigation-container__title {
		padding: 12px 0;
	}
	.description__wrapper {
		max-height: 90px;
		overflow-y: hidden;
	}
	.brands-expander {
		display: none;
	}
	.service-expander {
		display: none;
	}
	.service-expander__icon {
		display: none;
	}
	main {
		margin-top: 16px;
	}
	.info__title-line {
		transform: translate(-16px, 0px);
	}
	.footer__copyrights {
		flex-direction: column;
		justify-content: center;
		a {
			display: block;
			width: 100%;
			text-align: center;
		}
		p {
			display: block;
			width: 100%;
			text-align: center;
		}
	}
	.feedback__drawer {
		margin: 0 auto;
		padding: 16px 16px;
		width: 304px;
		.navigation-button {
			position: relative;
		}
	}
	.call__drawer {
